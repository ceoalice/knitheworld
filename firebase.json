{
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [ 
      {
      "source": "/home",
      "destination": "/index.html"
      }, {
      "source": "/gui",
      "destination": "/gui.html"
      }, {
      "source": "/users/**",
      "function": "user"
      }, {
      "source": "/projects/**",
      "function": "project"
    } ]
  }
}
